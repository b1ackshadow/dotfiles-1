// $XFree86: xc/programs/xkbcomp/symbols/proylatin,v 1.2 2002/11/22 04:03:28 dawes Exp $

partial default alphanumeric_keys
xkb_symbols "basic" {

    #include "pc(common)"

    name[Group1]="Universal Esperanto (Proycon)";

    key <TLDE>	{ [ apostrophe, asciitilde,    grave,   asciitilde ]	};
    key <LSGT>	{ [ apostrophe, asciitilde,    grave,   asciitilde ]}; //for mac keyboards

    key <AE01>	{ [         1,     exclam,      exclamdown,   exclamdown ]	};
    key <AE02>	{ [         2,     at,      quotedbl,   quotedbl ]	};
    key <AE03>	{ [         3,     numbersign, periodcentered,   sterling ]	};
    key <AE04>	{ [         4,     dollar,   EuroSign,       sterling ]	};
    key <AE05>  { [	    5,	   percent, EuroSign ] };
    key <AE06>	{ [         6,     asciicircum, notsign  ]	};
    key <AE07>	{ [         7,     ampersand, braceleft  ]	};
    key <AE08>	{ [         8,     asterisk, asterisk  ]	};
    key <AE09>	{ [         9,     parenleft, bracketleft, braceleft  ]	};
    key <AE10>	{ [         0,     parenright, bracketright, braceright  ]	};
    key <AE11>	{ [minus,   underscore,    braceleft, braceleft ]	};
    key <AE12>	{ [equal, plus, braceright,   braceright ]};

    key <AD01> {        [         q,    Q               ]       };
    key <AD02> {        [         w,    W               ]       };
    key <AD03> {        [         e,    E   ,ae, AE            ]       };
    key <AD04> {        [         r,    R               ]       };
    key <AD05> {        [         t,    T               ]       };
    key <AD06> {        [         y,    Y               ]       };
    key <AD07> {        [         u,    U ,ubreve, Ubreve   ]       };
    key <AD08> {        [         i,    I , idotless , Iabovedot     ]       };
    key <AD09> {        [         o,    O ,oslash , oe              ]       };
    key <AD10> {        [         p,    P              ]       };


    key <AD11>	{ [dead_grave, dead_circumflex, dead_caron, dead_breve ] };
    key <AD12>	{ [      dead_tilde,   dead_diaeresis, dead_macron,  dead_abovedot ]	};

    key <AC01> {        [         a,    A , aring, Aring              ]       };
    key <AC02> {        [         s,    S , scircumflex, Scircumflex ]       };
    key <AC03> {        [         d,    D               ]       };
    key <AC04> {        [         f,    F               ]       };
    key <AC05> {        [         g,    G , gcircumflex, Gcircumflex      ]   };
    key <AC06> {        [         h,    H , hcircumflex, Hcircumflex   ]       };
    key <AC07> {        [         j,    J  , jcircumflex, Jcircumflex   ]       };
    key <AC08> {        [         k,    K               ]       };
    key <AC09> {        [         l,    L , lstroke, Lstroke             ]       };
    key <AC10> {        [ semicolon,    colon           ]       };
    key <AC11> {        [ apostrophe,   quotedbl        ]       };

    key <AC10>	{ [    semicolon, colon, ntilde, Ntilde ]	};
    key <AC11>	{ [dead_acute, dead_diaeresis, apostrophe,   quotedbl ]	};

    key <BKSL>	{ [  backslash,   bar,   ccedilla,   Ccedilla ]	};

    key <AB01> {        [         z,    Z               ]       };
    key <AB02> {        [         x,    X               ]       };
    key <AB03> {        [         c,    C, ccircumflex, Ccircumflex     ]       };
    key <AB04> {        [         v,    V               ]       };
    key <AB05> {        [         b,    B , ssharp, Ssharp             ]       };
    key <AB06> {        [         n,    N , ntilde, Ntilde             ]       };
    key <AB07> {        [         m,    M               ]       };
    key <AB08> {        [     comma,    less            ]       };
    key <AB09> {        [    period,    greater         ]       };
    key <AB10> {        [     slash,    question , questiondown, questiondown       ]       };

    replace key <PRSC> { [ ISO_Level3_Shift ] };

    include "level3(ralt_switch)"
    include "level3(rwin_switch)"
    include "ctrl(nocaps)"
};
